[user]
        name = RobertGorecki
        email = robertgorecki86@gmail.com
[alias]
        ci = commit
        st = status
        co = checkout
        br = branch
		amend = commit -a --amend
        unstage = reset HEAD
        undocommit=reset HEAD~
[color]
        ui = auto
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
[color "status"]
        added = yellow
        changed = green
        untracked = cyan
[push]
        default = current
